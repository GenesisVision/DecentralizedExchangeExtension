{
    "manifest_version": 2,
    "name": "Decentralize Exchange Extension",
    "version": "1.0",
    
    "icons": {
      "128": "smile.png"
    },
    "content_scripts": [
      {
        "matches": [ "*://*/*", "http://localhost/*"],
        "js": [
            "jquery-3.3.1.min.js",
            "content.js"
          ],
          "run_at": "document_start",
          "all_frames": true
      }
    ],
    "web_accessible_resources": [
        "web3interceptor.js","web3.js/dist/web3.min.js"
      ],
    "background": {
      "scripts": [ "jquery-3.3.1.min.js",  "config.js" , "background.js"]
    },
    "permissions": [
        "webRequestBlocking",
        "webRequest",
        "https://*.infura.io/*",
        "http://localhost:54076/dexchange/Node",
    ],
  
        "browser_action": {
          "default_title": "Decentralize Exchange Extension",
          "default_icon": "smile.png",
          "default_popup": "index.html"
      }
      
  }